{
	"name": "revuc-sveltekit-admin",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"encrypt": "dotenvx encrypt -f .env.development -f .env.production",
		"decrypt": "dotenvx decrypt -f .env.development -f .env.production",
		"dev": "dotenvx run -f .env.development -- vite dev",
		"build": "dotenvx run -f .env.production -- vite build",
		"preview": "dotenvx run -f .env.production -- vite build && wrangler dev",
		"db:gen": "drizzle-kit generate",
		"db:seed": "dotenvx run -f .env.development -- tsx ./src/lib/server/db/seed.ts",
		"db:migrate": "dotenvx run -f .env.development -- drizzle-kit migrate",
		"db:studio": "dotenvx run -f .env.development -- drizzle-kit studio",
		"db:migrate:prod": "dotenvx run -f .env.production -- drizzle-kit migrate",
		"db:studio:prod": "dotenvx run -f .env.production -- drizzle-kit studio"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250927.0",
		"@dotenvx/dotenvx": "^1.51.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.13",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/node": "^22.18.6",
		"drizzle-kit": "^0.30.6",
		"drizzle-seed": "^0.3.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.6",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.6",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"wrangler": "^4.40.2"
	},
	"dependencies": {
		"@iconify-json/simple-icons": "^1.2.53",
		"@iconify-json/tabler": "^1.2.23",
		"@libsql/client": "^0.14.0",
		"csv-parse": "^6.1.0",
		"csv-stringify": "^6.6.0",
		"daisyui": "^5.1.24",
		"drizzle-orm": "^0.40.1",
		"nanoid": "^5.1.6",
		"unplugin-icons": "^22.3.0",
		"valibot": "^1.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
